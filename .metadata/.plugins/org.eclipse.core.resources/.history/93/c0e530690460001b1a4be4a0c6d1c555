package variaveis;

import javax.swing.JOptionPane;

public class TiposPrimitivos {

	public static void main(String[] args) {
		
		String marca = JOptionPane.showInputDialog("Informe a marca da Televisão"); 
		double preco = Double.parseDouble(JOptionPane.showInputDialog("Informe o preço da televisão"));
		int polegadas = Integer.parseInt(JOptionPane.showInputDialog("Informe quantas polegadas e Televisão possui"));
		int voltagem = Integer.parseInt(JOptionPane.showInputDialog("Informe a voltagem da televisão"));
		int estoque = Integer.parseInt(JOptionPane.showInputDialog("Informe a quantidade em estoque"));
		

	}

}
