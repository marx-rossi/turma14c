package br.com.ecommerce.beans;

public class ClientePF extends Cliente {
	
	private String cpf;
	private String rg;
	
	public ClientePF() {
		super();
	}
	
	
	public ClientePF(String nome, Endereco endereco, String telefone, String cpf, String rg) {
		super(nome, endereco, telefone);
		this.cpf = cpf;
		this.rg = rg;
	}


	
	

	public void setAll(String nome, Endereco endereco, String telefone, String cpf, String rg) {
		super.setAll(nome, endereco, telefone);
		this.cpf = cpf;
		this.rg = rg;
	}


	public String getAll() {
		return "ClientePF [cpf=" + cpf + ", rg=" + rg + "," + super.getAll() + "]"; //super.getAll() retorna os valores da super classe Cliente
	}


	public String getPf() {
		return cpf;
	}
	public void setPf(String pf) {
		this.cpf = cpf;
	}
	public String getRg() {
		return rg;
	}
	public void setRg(String rg) {
		this.rg = rg;
	}
	
		
	
	
	
	
	

}
