package variaveis;

public class TiposDados {
	
	public static void main(String[] args) {
		String nome = "Karl Marx";
		int idade = 203;
		double altura = 1.75;
		System.out.println("Nome: " + nome);
		System.out.println("Idade: " + idade);
		System.out.println("Altura: " + altura + "metros.");
		
	}

}
